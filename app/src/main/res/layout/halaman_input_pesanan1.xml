<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:outlineProvider="none"
    app:layout_insetEdge="bottom">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarInput"
        android:layout_width="419dp"
        android:layout_height="70dp"
        android:background="#61A2F9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <com.google.android.material.appbar.MaterialToolbar
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:title="Pesanan"
            app:titleTextColor="#F1EEEE"
            tools:layout_editor_absoluteX="18dp" />

    </androidx.appcompat.widget.Toolbar>


    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewPesanan"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="52dp"
        android:outlineProvider="none"
        app:cardBackgroundColor="@android:color/transparent"
        app:cardCornerRadius="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarInput">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:outlineProvider="none">


            <Button
                android:id="@+id/btn_backhome"
                android:layout_width="120dp"
                android:layout_height="48dp"
                android:layout_marginStart="72dp"
                android:layout_marginTop="60dp"
                android:text="BATAL"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                app:backgroundTint="#365E7D"
                app:cornerRadius="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etjumlahcucian" />

            <Button
                android:id="@+id/btn_lanjutinput2"
                android:layout_width="120dp"
                android:layout_height="48dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="60dp"
                android:text="LANJUT"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                app:backgroundTint="#365E7D"
                app:cornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etjumlahcucian" />

            <EditText
                android:id="@+id/etTelp"
                android:layout_width="340dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:ems="10"
                android:hint="Nomor Telepon"
                android:inputType="text|number"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.254" />

            <ImageView
                android:id="@+id/btn_tglselesai"
                android:layout_width="32dp"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="@+id/text_tanggalselesai"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.841"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/text_tanggalselesai"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/kalendar" />

            <TextView
                android:id="@+id/text_tanggalselesai"
                android:layout_width="339dp"
                android:layout_height="33dp"
                android:layout_gravity="center"
                android:ems="10"
                android:hint="Tanggal Selesai"
                android:inputType="text"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.458" />

            <EditText
                android:id="@+id/etjeniscucian"
                android:layout_width="340dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:ems="10"
                android:hint="Jenis Cucian"
                android:inputType="text"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.554" />

            <EditText
                android:id="@+id/etjumlahcucian"
                android:layout_width="340dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:ems="10"
                android:hint="Jumlah Cucian"
                android:inputType="number"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.654" />

            <EditText
                android:id="@+id/etNamaPelanggan"
                android:layout_width="340dp"
                android:layout_height="52dp"
                android:layout_gravity="center"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:ems="10"
                android:hint="Nama Pelanggan"
                android:inputType="textPersonName"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.054" />

            <!--            <EditText-->
            <!--                android:id="@+id/text_jenispaket"-->
            <!--                android:layout_width="256dp"-->
            <!--                android:layout_height="50dp"-->
            <!--                android:layout_gravity="center"-->
            <!--                android:ems="10"-->
            <!--                android:hint="Jenis Paket"-->
            <!--                android:inputType="textPassword"-->
            <!--                android:textColor="#365E7D"-->
            <!--                android:textColorHighlight="#000000"-->
            <!--                android:textColorHint="#000000"-->
            <!--                android:textColorLink="#000000"-->
            <!--                app:layout_constraintBottom_toBottomOf="parent"-->
            <!--                app:layout_constraintEnd_toEndOf="parent"-->
            <!--                app:layout_constraintHorizontal_bias="0.496"-->
            <!--                app:layout_constraintStart_toStartOf="parent"-->
            <!--                app:layout_constraintTop_toTopOf="parent"-->
            <!--                app:layout_constraintVertical_bias="0.754" />-->


            <EditText
                android:id="@+id/etAlamat"
                android:layout_width="340dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:ems="10"
                android:hint="Alamat"
                android:inputType="text"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.154" />

            <ImageView
                android:id="@+id/btn_tglpesan"
                android:layout_width="32dp"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="@+id/text_tanggalpesan"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.841"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/text_tanggalpesan"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/kalendar" />

            <TextView
                android:id="@+id/text_tanggalpesan"
                android:layout_width="335dp"
                android:layout_height="39dp"
                android:hint="Tanggal Pesan"
                android:outlineProvider="none"
                android:textColor="#000000"
                android:textColorHighlight="#000000"
                android:textColorHint="#000000"
                android:textColorLink="#000000"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.473"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.364" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>