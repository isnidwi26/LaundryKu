        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.767" />

    <Button
        android:id="@+id/button_lanjut2"
        android:layout_width="120dp"
        android:layout_height="47dp"
        android:text="LANJUT"
